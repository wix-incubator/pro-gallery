{
  "name": "pro-playground",
  "version": "5.0.72",
  "private": true,
  "scripts": {
    "watch": "sleep 10 && node scripts/start.js",
    "start": "PORT=3006 node scripts/start.js",
    "build": "node scripts/build.js",
    "unit": "node scripts/test.js",
    "test": "echo 'no test in wix CI'",
    "deploy": "node scripts/build.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "start-playground": "node scripts/start.js"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "7.16.7",
    "antd": "^4.19.1",
    "css-animation": "1.5.0",
    "firebase": "^8.2.4",
    "image-client-api": "^1.3130.0",
    "lodash.clonedeep": "^4.5.0",
    "moment": "2.29.2",
    "pro-gallery": "5.0.72",
    "pro-gallery-blueprints": "5.0.72",
    "pro-gallery-lib": "5.0.72",
    "pro-layouts": "5.0.72",
    "re-resizable": "^6.8.0",
    "react": "^18.2.0",
    "react-color": "^2.17.3",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.4.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^4.3.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.6.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^2.1.1",
    "eslint": "8.54.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-watch-typeahead": "^2.2.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-forge": "1.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^12.0.0",
    "sass": "^1.59.3",
    "sass-loader": "10.2.0 ",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.38.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "resolutions": {
    "css-animation": "1.5.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "homepage": "https://pro-gallery.surge.sh/"
}
