{
  "publishScoped": false,
  "private": false,
  "name": "pro-gallery",
  "version": "1.5.49",
  "author": {
    "name": "Guy Sopher",
    "email": "guysopher@gmail.com"
  },
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "PORT=9837 yoshi start",
    "start": "PORT=9837 yoshi start",
    "build": "yoshi build",
    "build-svg": "svg2react-icon src/assets/images/svg-src src/components/svgs",
    "precommit": " ",
    "pretest": "yoshi build",
    "test": "jest --testPathIgnorePatterns '.*/e2e/.*.e2e.spec.js'",
    "posttest": "yoshi lint",
    "release": "yoshi release",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "start-with-test": "PORT=9837 yoshi start --with-test",
    "e2e-test": "jest --testPathPattern='.*/e2e/.*.e2e.spec.js'"
  },
  "dependencies": {
    "@vimeo/player": "2.8.2",
    "jest-image-snapshot": "^2.11.1",
    "pro-layouts": "^1.5.35",
    "react-player": "1.10.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-spies": "^0.7.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15.4": "^1.0.4",
    "enzyme-adapter-react-16": "^1.7.1",
    "file-loader": "^4.1.0",
    "husky": "^0.13.4",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "match-screenshot": "^1.0.14",
    "nock": "^9.3.3",
    "node-sass": "^4.12.0",
    "puppeteer": "^1.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.1.0",
    "sinon": "^6.0.0",
    "svg-inline-loader": "^0.8.0",
    "svg-react-loader": "^0.4.6",
    "svg2react-icon": "^3.0.91",
    "wallaby-webpack": "^3.9.9",
    "yoshi": "^4.10.1",
    "yoshi-style-dependencies": "^4.0.0"
  },
  "babel": {
    "presets": [
      "yoshi"
    ]
  },
  "eslintConfig": {
    "extends": "yoshi",
    "rules": {
      "import/no-unresolved": "off",
      "default-case": "off",
      "prettier/prettier": "off"
    }
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "yoshi": {
    "entry": {
      "main": "./index"
    },
    "servers": {
      "cdn": {
        "port": 9999
      }
    },
    "externals": {
      "babel-runtime": "babel-runtime",
      "react": "React",
      "react-dom": "ReactDOM"
    },
    "exports": "pro-gallery"
  },
  "jest": {
    "moduleNameMapper": {
      "^-!svg-react-loader.*$": "<rootDir>/__testsDrivers__/svgImportMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist"
    ]
  }
}
