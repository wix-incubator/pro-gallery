{
  "publishScoped": false,
  "private": false,
  "name": "pro-gallery",
  "version": "2.0.4",
  "sideEffects": [
    "./src/components/styles/gallery.scss",
    "./src/components/gallery/leanGallery/leanGallery.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/pro-gallery.git"
  },
  "author": {
    "name": "Guy Sopher",
    "email": "guysopher@gmail.com"
  },
  "main": "dist/src/index.js",
  "module": "dist/es/src/index.js",
  "files": [
    "dist",
    "lib.js"
  ],
  "scripts": {
    "watch": "PORT=9837 yoshi start",
    "start": "PORT=9837 yoshi start",
    "build": "yoshi build",
    "precommit": " ",
    "pretest": "yoshi build",
    "test": "jest --testPathIgnorePatterns '.*/e2e/.*.e2e.spec.js' --silent",
    "posttest": "yoshi lint",
    "release": "yoshi release",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "start-with-test": "PORT=9837 yoshi start --with-test",
    "e2e-test": "jest --testPathPattern='.*/e2e/.*.e2e.spec.js'"
  },
  "dependencies": {
    "@vimeo/player": "2.8.2",
    "hls.js": "^0.13.2",
    "pro-layouts": "2.0.4",
    "react-player": "^1.15.3"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-spies": "^0.7.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15.4": "^1.0.4",
    "enzyme-adapter-react-16": "^1.7.1",
    "file-loader": "^4.1.0",
    "husky": "^0.13.4",
    "jest-image-snapshot": "^2.11.1",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "match-screenshot": "^1.0.14",
    "nock": "^9.3.3",
    "node-sass": "^4.13.1",
    "puppeteer": "^1.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.1.0",
    "sinon": "^6.0.0",
    "wallaby-webpack": "^3.9.9",
    "yoshi": "~4.25.0",
    "yoshi-style-dependencies": "^4.0.0"
  },
  "babel": {
    "presets": [
      "yoshi"
    ]
  },
  "eslintConfig": {
    "extends": "yoshi",
    "rules": {
      "import/no-unresolved": "off",
      "default-case": "off",
      "prettier/prettier": "off"
    }
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "yoshi": {
    "entry": {
      "main": "./index"
    },
    "servers": {
      "cdn": {
        "port": 9999
      }
    },
    "externals": {
      "babel-runtime": "babel-runtime",
      "react": "React",
      "react-dom": "ReactDOM"
    },
    "exports": "pro-gallery"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/drivers/mocks/stylesImportMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist"
    ]
  }
}
