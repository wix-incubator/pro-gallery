{
  "name": "pro-gallery",
  "version": "5.0.44",
  "sideEffects": [
    "./src/components/styles/gallery.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/pro-gallery.git"
  },
  "author": {
    "name": "Guy Sopher",
    "email": "guysopher@gmail.com"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm-types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "yarn run start",
    "build": " tsc && tsc -p tsconfig-esm.json && yarn run css-compile",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css src/components/styles/gallery.scss:dist/statics/main.css",
    "start": "concurrently \"yarn run css-watch\" \"yarn run code-watch\"",
    "code-watch": "tsc && tsc -w -p tsconfig-esm-dev.json",
    "css-watch": "sass --watch src/components/styles/gallery.scss:dist/statics/main.css",
    "unit": "jest --testPathIgnorePatterns '.*/e2e/.*.e2e.spec.js' --silent",
    "test": "echo 'no test in wix CI'",
    "test:e2e-all": "jest --testPathPattern='.*/e2e/.*.e2e.spec.js'",
    "test:e2e-layouts": "jest --testPathPattern='.*/e2e/layouts/.*.e2e.spec.js'",
    "test:e2e-styleParams": "jest --testPathPattern='.*/e2e/styleParams/.*.e2e.spec.js'",
    "test:e2e-integration": "jest --testPathPattern='.*/e2e/integration/.*.e2e.spec.js'",
    "lint": "eslint \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn run lint --fix"
  },
  "dependencies": {
    "@vimeo/player": "2.8.2",
    "hls.js": "^1.1.5",
    "pro-gallery-blueprints": "workspace:*",
    "pro-gallery-lib": "workspace:*",
    "pro-layouts": "workspace:*",
    "react-player": "^2.6.2",
    "three": "0.143.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/react": "^16.14.6",
    "@types/three": "0.143.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babelify": "^10.0.0",
    "chai": "^4.2.0",
    "chai-spies": "^0.7.1",
    "concurrently": "^6.1.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-image-snapshot": "6.1.0",
    "jest-stare": "2.5.0",
    "prettier": "^2.1.2",
    "puppeteer": "19.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass": "^1.29.0",
    "sinon": "^6.0.0",
    "ts-jest": "29.0.5",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
